{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      },
      {
        "src": "./prisma",
        "use": "@prisma/cli",
        "config": { "cmd": "generate" }
      },
      {
        "src": "./prisma", 
        "use": "@prisma/cli",
        "config": { "cmd": "migrate" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  